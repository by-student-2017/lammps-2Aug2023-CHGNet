variable elem string "C H H" # specifies the potential file used

# set overlap_cutoff (Note: H-H: 1.06 A) = x 85% => 0.90
variable overlap_cutoff_value equal 0.90

#-------------------- Force field --------------------------------------------------------
# GPU case
pair_style hybrid/overlay chgnet/gpu /mnt/d/lammps/potentials/CHGNET momb 12.0 0.75 20.0
pair_coeff    * *  chgnet/gpu 0.3.0 ${elem}
pair_coeff    * *  chgnet/gpu 0.3.0 C H H

# CPU case
#pair_style hybrid/overlay chgnet /mnt/d/lammps/potentials/CHGNET momb 12.0 0.75 20.0
#pair_coeff    * *  chgnet 0.3.0 ${elem}

# atom type 1, C
variable C6_1 equal 1.75  # Jnm6mol-1
variable R0_1 equal 1.452 # Angstrom

# atom type 2, H
variable C6_2 equal 0.14  # Jnm6mol-1
variable R0_2 equal 1.001 # Angstrom

# atom type 3, H
variable C6_3 equal 0.14  # Jnm6mol-1
variable R0_3 equal 1.001 # Angstrom

# convert [Jnm6mol-1] to [eVA6], (sqrt() or ()^0.5)
#-----------------------------------------------------------------------------------------
pair_coeff 1 1 momb 0.0 1.0 1.0 $(sqrt(v_C6_1 * v_C6_1)*10.364) $(v_R0_1 + v_R0_1) # C-C
pair_coeff 1 1 momb 0.0 1.0 1.0 18.137000000000000455 $(v_R0_1 + v_R0_1) 
pair_coeff 1 1 momb 0.0 1.0 1.0 18.137000000000000455 2.9039999999999999147 
pair_coeff 1 2 momb 0.0 1.0 1.0 $(sqrt(v_C6_1 * v_C6_2)*10.364) $(v_R0_1 + v_R0_2) # C-H
pair_coeff 1 2 momb 0.0 1.0 1.0 5.1299182761521660012 $(v_R0_1 + v_R0_2) 
pair_coeff 1 2 momb 0.0 1.0 1.0 5.1299182761521660012 2.4529999999999998472 
pair_coeff 1 3 momb 0.0 1.0 1.0 $(sqrt(v_C6_1 * v_C6_3)*10.364) $(v_R0_1 + v_R0_3) # C-Hg
pair_coeff 1 3 momb 0.0 1.0 1.0 5.1299182761521660012 $(v_R0_1 + v_R0_3) 
pair_coeff 1 3 momb 0.0 1.0 1.0 5.1299182761521660012 2.4529999999999998472 
#-----------------------------------------------------------------------------------------
pair_coeff 2 2 momb 0.0 1.0 1.0 $(sqrt(v_C6_2 * v_C6_2)*10.364) $(v_R0_2 + v_R0_2) # H-H
pair_coeff 2 2 momb 0.0 1.0 1.0 1.4509600000000002495 $(v_R0_2 + v_R0_2) 
pair_coeff 2 2 momb 0.0 1.0 1.0 1.4509600000000002495 2.0019999999999997797 
pair_coeff 2 3 momb 0.0 1.0 1.0 $(sqrt(v_C6_2 * v_C6_3)*10.364) $(v_R0_2 + v_R0_3) # H-Hg
pair_coeff 2 3 momb 0.0 1.0 1.0 1.4509600000000002495 $(v_R0_2 + v_R0_3) 
pair_coeff 2 3 momb 0.0 1.0 1.0 1.4509600000000002495 2.0019999999999997797 
#-----------------------------------------------------------------------------------------
pair_coeff 3 3 momb 0.0 1.0 1.0 $(sqrt(v_C6_3 * v_C6_3)*10.364) $(v_R0_3 + v_R0_3) # Hg-Hg
pair_coeff 3 3 momb 0.0 1.0 1.0 1.4509600000000002495 $(v_R0_3 + v_R0_3) 
pair_coeff 3 3 momb 0.0 1.0 1.0 1.4509600000000002495 2.0019999999999997797 
